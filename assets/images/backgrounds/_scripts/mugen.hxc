import Paths;
import flixel.FlxSprite;
import states.PlayState;

function create() {
    PlayState.defaultCamZoom = 0.7;

    dadPos.x -= 100;
    dadCam.y += 150;

    this.gfVersion = "no-gf";

    var bg = new FlxSprite(-640, -1000).loadGraphic(Paths.image("backgrounds/mugen/mugen"));
    add(bg);
}